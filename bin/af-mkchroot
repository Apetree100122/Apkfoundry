import logging  # basicConfig
import sys      # argv, exit

import apkfoundry.container # cont_make

logging.basicConfig(
    level=logging.DEBUG,
    format="%(asctime)s %(levelname)s %(message)s",
)

cont = apkfoundry.container.cont_make(sys.argv[1:])
if not cont:
    sys.exit(1)
