import logging # basicConfig

from apkfoundry.irc import AFIRCProtocol

logging.basicConfig(
    level=logging.DEBUG,
    format="%(asctime)s %(levelname)s [%(threadName)s] %(message)s",
)

bot = AFIRCProtocol()
bot.mqtt_init()
bot.loop()
